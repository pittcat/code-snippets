{
    "h1": {
        "prefix": "h1",
        "body": "# ",
        "description": "Header 1"
    },
    "h2": {
        "prefix": "h2",
        "body": "## ",
        "description": "Header 2"
    },
    "h3": {
        "prefix": "h3",
        "body": "### ",
        "description": "Header 3"
    },
    "h4": {
        "prefix": "h4",
        "body": "#### ",
        "description": "Header 4"
    },
    "code": {
        "prefix": "code",
        "body": "```${1:language}\n```",
        "description": "Code block"
    },
    "bold": {
        "prefix": "bold",
        "body": "**${1:body}**",
        "description": "Bold body"
    },
    "img center": {
        "prefix": "imgct",
        "body": "<p align = 'center'>\n<img src = '${1:Image Path}'>\n</p>\n<p align = 'center'>\n${2:Title}\n</p>",
        "description": "Insert an image at center"
    },
    "center latex code": {
        "prefix": "cltx",
        "body": "$$\n${1:latex}\n$$",
        "description": "Code block"
    },
    "line latex code": {
        "prefix": "lltx",
        "body": "$${1:latex}$",
        "description": "Code block"
    }
}
